# Based on eevee/rc/.Xcompose <https://github.com/eevee/rc/blob/73b2723/.XCompose>
include "%L"

# Letters with a macron
<Multi_key> <minus> <A>  : "Ā" U0100 # LATIN CAPITAL LETTER A WITH MACRON
<Multi_key> <minus> <E>  : "Ē" U0112 # LATIN CAPITAL LETTER E WITH MACRON
<Multi_key> <minus> <I>  : "Ī" U012A # LATIN CAPITAL LETTER I WITH MACRON
<Multi_key> <minus> <O>  : "Ō" U014C # LATIN CAPITAL LETTER O WITH MACRON
<Multi_key> <minus> <U>  : "Ū" U016A # LATIN CAPITAL LETTER U WITH MACRON
<Multi_key> <minus> <a>  : "ā" U0101 # LATIN SMALL LETTER A WITH MACRON
<Multi_key> <minus> <e>  : "ē" U0113 # LATIN SMALL LETTER E WITH MACRON
<Multi_key> <minus> <i>  : "ī" U012B # LATIN SMALL LETTER I WITH MACRON
<Multi_key> <minus> <o>  : "ō" U014D # LATIN SMALL LETTER O WITH MACRON
<Multi_key> <minus> <u>  : "ū" U016B # LATIN SMALL LETTER U WITH MACRON
<Multi_key> <minus> <AE> : "Ǣ" U01E2 # LATIN CAPITAL LETTER AE WITH MACRON
<Multi_key> <minus> <ae> : "ǣ" U01E3 # LATIN SMALL LETTER AE WITH MACRON
<Multi_key> <minus> <Y>  : "Ȳ" U0232 # LATIN CAPITAL LETTER Y WITH MACRON
<Multi_key> <minus> <y>  : "ȳ" U0233 # LATIN SMALL LETTER Y WITH MACRON
<Multi_key> <minus> <G>  : "Ḡ" U1E20 # LATIN CAPITAL LETTER G WITH MACRON
<Multi_key> <minus> <g>  : "ḡ" U1E21 # LATIN SMALL LETTER G WITH MACRON

# Arrows
<Multi_key> <less> <minus>          : "←" U2190
<Multi_key> <minus> <less>          : "←" U2190
<Multi_key> <minus> <greater>       : "→" U2192
<Multi_key> <less> <greater>        : "↔" U2194
<Multi_key> <circumflex> <bar>      : "↑" U2191
<Multi_key> <bar> <circumflex>      : "↑" U2191
<Multi_key> <v> <bar>               : "↓" U2193
<Multi_key> <bar> <v>               : "↓" U2193
<Multi_key> <bar> <less> <minus>    : "↚" U219A # LEFTWARDS ARROW WITH STROKE
<Multi_key> <bar> <minus> <greater> : "↛" U219B # RIGHTWARDS ARROW WITH STROKE
<Multi_key> <bar> <less> <greater>  : "↮" U21AE # LEFT RIGHT ARROW WITH STROKE

# Punctuation
<Multi_key> <asterisk> <asterisk> : "•" U2022 # BULLET
<Multi_key> <asterisk> <o>        : "◦" U25E6 # WHITE BULLET
<Multi_key> <asterisk> <greater>  : "‣" U2023 # TRIANGULAR BULLET
<Multi_key> <greater> <period>    : "›" U203A # SINGLE RIGHT-POINTING ANGLE QUOTATION MARK
<Multi_key> <less> <period>       : "‹" U2039 # SINGLE LEFT-POINTING ANGLE QUOTATION MARK
<Multi_key> <bar> <bar>           : "¦" brokenbar
<Multi_key> <plus> <bar>          : "†" U2020 # DAGGER
# Xorg default is '#' (pound)
<Multi_key> <plus> <plus>         : "‡" U2021 # DOUBLE DAGGER
<Multi_key> <asterisk> <3>        : "⁂" U2042 # ASTERISM

# Spaces and soft hyphens
<Multi_key> <n> <b> <s> <p> : " " nobreakspace
<Multi_key> <space> <0>     : "​" U200B # ZERO WIDTH SPACE
<Multi_key> <z> <w> <s> <p> : "​" U200B
<Multi_key> <minus> <space> : "­" U00AD # SOFT HYPHEN
<Multi_key> <s> <h> <y>     : "­" U00AD

# Misc goofy stuff
<Multi_key> <n> <l>                          : "␤" U2424 # SYMBOL FOR NEWLINE
<Multi_key> <bracketleft> <bracketright>     : "☑" U2611 # BALLOT BOX WITH CHECK
<Multi_key> <bracketleft> <x> <bracketright> : "☒" U2612 # BALLOT BOX WITH X
<Multi_key> <period> <period>                : "…" U2026 # HORIZONTAL ELLIPSIS
<Multi_key> <V> <V>                          : "✓" U2713 # CHECK MARK
<Multi_key> <X> <X>                          : "✗" U2717 # BALLOT X

# Mathematical symbols
<Multi_key> <colon> <period>                : "⫶" U2AF6 # TRIPLE COLON OPERATOR 
<Multi_key> <asciitilde> <asciitilde>       : "≈" U2248 # ALMOST EQUAL TO
<Multi_key> <exclam> <asciitilde> <asciitilde> : "≉" U2249 # NOT ALMOST EQUAL TO
<Multi_key> <slash> <equal>                 : "≠" U2260 # NOT EQUAL TO
<Multi_key> <equal> <slash>                 : "≠" U2260
<Multi_key> <exclam> <equal>                : "≠" U2260
<Multi_key> <less> <equal>                  : "≤" U2264 # LESS-THAN OR EQUAL TO
<Multi_key> <greater> <equal>               : "≥" U2265 # GREATER-THAN OR EQUAL TO
<Multi_key> <exclam> <less>                 : "≮" U226E # NOT LESS-THAN
<Multi_key> <exclam> <greater>              : "≯" U226F # NOT GREATER-THAN
<Multi_key> <equal> <equal>                 : "≡" U2261 # IDENTICAL TO
<Multi_key> <asciitilde> <equal>            : "≅" U2245 # APPROXIMATELY EQUAL TO
<Multi_key> <parenleft> <minus>             : "∈" U2208 # ELEMENT OF
<Multi_key> <i> <n>                         : "∈" U2208
<Multi_key> <parenleft> <slash> <minus>     : "∉" U2209 # NOT AN ELEMENT OF
<Multi_key> <exclam> <parenleft> <minus>    : "∉" U2209
<Multi_key> <exclam> <i> <n>                : "∉" U2209
<Multi_key> <parenleft> <parenleft>         : "⊂" U2282 # SUBSET OF
<Multi_key> <parenright> <parenright>       : "⊂" U2283 # SUPERSET OF
<Multi_key> <parenleft> <slash> <parenleft> : "⊄" U2284 # NOT A SUBSET OF
<Multi_key> <exclam> <parenleft> <parenleft> : "⊄" U2284
<Multi_key> <parenright> <slash> <parenright> : "⊄" U2285 # NOT A SUPERSET OF
<Multi_key> <minus> <slash>                 : "√" U221A # SQUARE ROOT
<Multi_key> <1> <slash> <0>                 : "∞" U221E # INFINITY
<Multi_key> <2> <slash> <0>                 : "ℵ" U2135 # ALEF SYMBOL
<Multi_key> <percent> <O>                   : "‱" U2031 # CLOCKWISE INTEGRAL
<Multi_key> <period> <colon>                : "∴" U2234 # THEREFORE
<Multi_key> <slash> <underscore>            : "∠" U2220 # ANGLE
<Multi_key> <exclam> <3>                    : "ℇ" U2107 # EULER CONSTANT
<Multi_key> <f> <comma>                     : "ƒ" U0192 # LATIN SMALL LETTER F WITH HOOK

# Greek letters
<Multi_key> <ampersand> <A>                 : "Α" U0391 # alpha
<Multi_key> <ampersand> <a>                 : "α" U03B1
<Multi_key> <ampersand> <B>                 : "Β" U0392 # beta
<Multi_key> <ampersand> <b>                 : "β" U03B2
<Multi_key> <ampersand> <G>                 : "Γ" U0393 # gamma
<Multi_key> <ampersand> <g>                 : "γ" U03B3
<Multi_key> <ampersand> <D>                 : "Δ" U03B4 # delta
<Multi_key> <ampersand> <d>                 : "δ" U03B4
<Multi_key> <ampersand> <E>                 : "Ε" U0395 # epsilon
<Multi_key> <ampersand> <e>                 : "ε" U03B5
<Multi_key> <ampersand> <Z>                 : "Ζ" U0396 # zeta
<Multi_key> <ampersand> <z>                 : "ζ" U03B6
<Multi_key> <ampersand> <H>                 : "Η" U0397 # eta
<Multi_key> <ampersand> <h>                 : "η" U03B7
<Multi_key> <ampersand> <H>                 : "Θ" U0398 # theta
<Multi_key> <ampersand> <h>                 : "θ" U03B8
<Multi_key> <ampersand> <I>                 : "Ι" U0399 # iota
<Multi_key> <ampersand> <i>                 : "ι" U03B9
<Multi_key> <ampersand> <K>                 : "Κ" U039A # kappa
<Multi_key> <ampersand> <k>                 : "κ" U03BA
<Multi_key> <ampersand> <L>                 : "Λ" U039B # lambda
<Multi_key> <ampersand> <l>                 : "λ" U03BB
<Multi_key> <ampersand> <M>                 : "Μ" U039C # mu
<Multi_key> <ampersand> <m>                 : "μ" U03BC
<Multi_key> <ampersand> <N>                 : "Ν" U039D # nu
<Multi_key> <ampersand> <n>                 : "ν" U03BD
<Multi_key> <ampersand> <X>                 : "Ξ" U039E # ksi
<Multi_key> <ampersand> <x>                 : "ξ" U03BE
<Multi_key> <ampersand> <O>                 : "Ο" U039F # omicron
<Multi_key> <ampersand> <o>                 : "ο" U03BF
<Multi_key> <ampersand> <P>                 : "Π" U03A0 # pi
<Multi_key> <ampersand> <p>                 : "π" U03C0
<Multi_key> <ampersand> <R>                 : "Ρ" U03A1 # rho
<Multi_key> <ampersand> <r>                 : "ρ" U03C1
<Multi_key> <ampersand> <S>                 : "Σ" U03A3 # sigma
<Multi_key> <ampersand> <s>                 : "σ" U03C3
<Multi_key> <ampersand> <$>                 : "ς" U03C2
<Multi_key> <ampersand> <T>                 : "Τ" U03A4 # tau
<Multi_key> <ampersand> <t>                 : "τ" U03C4
<Multi_key> <ampersand> <U>                 : "Υ" U03A5 # upsilon
<Multi_key> <ampersand> <u>                 : "υ" U03C5
<Multi_key> <ampersand> <F>                 : "Φ" U03A6 # phi
<Multi_key> <ampersand> <f>                 : "φ" U03C6
<Multi_key> <o> <bar>                       : "ϕ" U03D5
<Multi_key> <ampersand> <C>                 : "Χ" U03A7 # chi
<Multi_key> <ampersand> <c>                 : "χ" U03C7
<Multi_key> <ampersand> <W>                 : "Ψ" U03A8 # psi
<Multi_key> <ampersand> <w>                 : "ψ" U03C8
<Multi_key> <ampersand> <Q>                 : "Ω" U03A9 # omega
<Multi_key> <ampersand> <q>                 : "ω" U03C9

# Mathematical symbols that look like Greek letters
<Multi_key> <slash> <greater>               : "∆" U2206 # INCREMENT
<Multi_key> <plus> <plus>                   : "∑" U2211 # N-ARY SUMMATION
<Multi_key> <asterisk> <asterisk>           : "∏" U220F # N-ARY PRODUCT

